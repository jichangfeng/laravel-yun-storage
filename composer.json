{
    "name": "jichangfeng/laravel-yun-storage",
    "type": "library",
    "description": "Upload attachments to content storage platform like Aliyun OSS, Tencent COS",
    "homepage": "http://github.com/jichangfeng/laravel-yun-storage",
    "keywords": ["storage", "yun storage", "oss", "aliyun oss", "cos", "tencent cos"],
    "license": "MIT",
    "authors": [
        {
            "name": "Changfeng Ji",
            "email": "jichf@qq.com"
        }
    ],
    "require": {
        "php": ">=5.6",
        "jichangfeng/yun-storage": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "YunStorage\\Laravel\\": "./src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "YunStorage\\Laravel\\YunStorageServiceProvider"
            ],
            "aliases": {
                "YunStorageFacade": "YunStorage\\Laravel\\YunStorageFacade"
            }
        }
    }
}
